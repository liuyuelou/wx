.category-page .search-header {
  position: fixed;
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  left: 0;
  top: 6rpx;
  z-index: 999;
  border-bottom: 1px solid #f5f5f5;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 10rpx;
}

.category-page .search-header .icon1 {
  position: relative;
  height: 60rpx;
  line-height: 60rpx;
  text-align: left;
  color: #ccc;
  margin-right: 10rpx;
}

.category-page .search-header .search {
  width: 80%;
  height: 60rpx;
  background: #f5f5f5;
  border-radius: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  /* padding: 0 30rpx; */
  box-sizing: border-box;
  padding: 0 0 0 10rpx;
  text-indent: 6rpx;
}

.category-page .search-header .icon2 {
  position: relative;
  height: 60rpx;
  line-height: 60rpx;
  color: #666;
  margin-left: 20rpx;
  font-size: 40rpx;
}

.category-page .search-header .search .iconfont {
  color: #999;
  margin-right: 8rpx;
}

.category-page .search-header .search input {
  color: #999;
  font-size: 28rpx;
}

.zonghe-box {
  width: 100%;
  text-align: center;
  background-color: #fff;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1px solid #f1f1f1;
  font-size: 26rpx;
  margin-top: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.zonghe-box .zonghe1 {
  width: 148rpx;
  height: 100%;
  -webkit-box-flex: 1;
}

.zonghe-box .active {
  color: #f92028;
}
